@model Reports.Models.TotalLeads

@{
    ViewData["Title"] = "Total Leads";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form class="form-inline" asp-action="Index" asp-controller="TotalLeads" method="post">
    <div class="form-group">
        <label asp-for="Date1"></label>
        <input type="date" class="form-control mr-2" asp-for="Date1" />
    </div>
    <div class="form-group">
        <label asp-for="Date2"></label>
        <input type="date" class="form-control mr-2" asp-for="Date2" />
    </div>
    <div class="form-group">
        <input type="submit" value="Search" class="btn btn-primary" />
    </div>
</form>



